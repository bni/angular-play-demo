{
    "name": "angular-play-demo",
    "description": "Angular Play Demo",
    "version": "0.0.1",
    "dependencies": {
        "angular": "1.4.4",
        "angular-resource": "1.4.4",
        "angular-ui-router": "0.2.15",
        "angular-bootstrap": "0.13.3",
        "angular-tablesort": "1.1.0",
        "angular-breadcrumb": "0.4.1",
        "flat-ui": "2.2.2",
        "font-awesome": "4.4.0",
        "fastclick": "1.0.6"
    },
    "install": {
        "path": "app/vendors",
        "sources": {
            "angular": {
                "mapping": [
                    {"bower_components/angular/angular.min.js": "js/angular.min.js"},
                    {"bower_components/angular/angular.min.js.map": "js/angular.min.js.map"},
                    {"bower_components/angular-resource/angular-resource.min.js": "js/angular-resource.min.js"},
                    {"bower_components/angular-resource/angular-resource.min.js.map": "js/angular-resource.min.js.map"},
                    {"bower_components/angular-ui-router/release/angular-ui-router.min.js": "js/angular-ui-router.min.js"},
                    {"bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js": "js/ui-bootstrap-tpls.min.js"},
                    {"bower_components/angular-tablesort/js/angular-tablesort.js": "js/angular-tablesort.js"},
                    {"bower_components/angular-tablesort/tablesort.css": "css/tablesort.css"},
                    {"bower_components/angular-breadcrumb/release/angular-breadcrumb.min.js": "js/angular-breadcrumb.min.js"}
                ]
            },
            "flat-ui": {
                "mapping": [
                    {"bower_components/flat-ui/dist/css/vendor/bootstrap.min.css": "css/bootstrap.min.css"},
                    {"bower_components/flat-ui/dist/css/flat-ui.min.css": "css/flat-ui.min.css"},
                    {"bower_components/flat-ui/dist/fonts/glyphicons/flat-ui-icons-regular.eot": "fonts/glyphicons/flat-ui-icons-regular.eot"},
                    {"bower_components/flat-ui/dist/fonts/glyphicons/flat-ui-icons-regular.svg": "fonts/glyphicons/flat-ui-icons-regular.svg"},
                    {"bower_components/flat-ui/dist/fonts/glyphicons/flat-ui-icons-regular.ttf": "fonts/glyphicons/flat-ui-icons-regular.ttf"},
                    {"bower_components/flat-ui/dist/fonts/glyphicons/flat-ui-icons-regular.woff": "fonts/glyphicons/flat-ui-icons-regular.woff"},
                    {"bower_components/flat-ui/dist/fonts/lato/lato-regular.eot": "fonts/lato/lato-regular.eot"},
                    {"bower_components/flat-ui/dist/fonts/lato/lato-regular.svg": "fonts/lato/lato-regular.svg"},
                    {"bower_components/flat-ui/dist/fonts/lato/lato-regular.ttf": "fonts/lato/lato-regular.ttf"},
                    {"bower_components/flat-ui/dist/fonts/lato/lato-regular.woff": "fonts/lato/lato-regular.woff"}
                ]
            },
            "font-awesome": {
                "mapping": [
                    {"bower_components/font-awesome/css/font-awesome.min.css": "css/font-awesome.min.css"},
                    {"bower_components/font-awesome/fonts/fontawesome-webfont.eot": "fonts/fontawesome-webfont.eot"},
                    {"bower_components/font-awesome/fonts/fontawesome-webfont.svg": "fonts/fontawesome-webfont.svg"},
                    {"bower_components/font-awesome/fonts/fontawesome-webfont.ttf": "fonts/fontawesome-webfont.ttf"},
                    {"bower_components/font-awesome/fonts/fontawesome-webfont.woff": "fonts/fontawesome-webfont.woff"}
                ]
            }
        },
        "ignore": [
            "jquery",
            "angular-resource",
            "angular-ui-router",
            "angular-bootstrap",
            "angular-tablesort",
            "angular-breadcrumb",
            "bootstrap",
            "videojs"
        ]
    }
}
